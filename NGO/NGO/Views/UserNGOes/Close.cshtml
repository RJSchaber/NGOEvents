
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>

</head>
<body>
    <script type="text/javascript">

        function RefreshParent()
        {
            if (window.opener != null && !window.opener.closed)
            {
            window.opener.location.reload();
            }
        }

        window.onbeforeunload = RefreshParent;

        window.close();
    </script>
</body>
</html>
