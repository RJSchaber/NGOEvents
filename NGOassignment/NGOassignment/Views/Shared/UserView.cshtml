
@{
    Layout = null;
}

@Styles.Render("~/Content/UserView.css")

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - NGO PROJECT</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="sidebar">
        <ul>
            <li>@Html.ActionLink("UserManagement", "Index", "UserNGOes")</li>
            <li>@Html.ActionLink("EventManagement", "Index", "Home")</li>
            <li>@Html.ActionLink("UserView", "Index", "Home")</li>
        </ul>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
